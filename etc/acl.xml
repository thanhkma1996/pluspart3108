<?xml	version="1.0"?>
<config	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource	id="Magento_Backend::admin">
                <resource	id="Magento_Backend::stores">
                    <resource	id="Magento_Backend::stores_settings">
                        <resource	id="Magento_Config::config">
                            <resource	id="Magenest_Movie::config_movie" title="Movie Section"	/>
                        </resource>
                    </resource>
                </resource>
                <resource	id="Magenest_Movie::moviemenu" title="Magenest movie">
                        <resource	id="Magenest_Movie::movie"	title="Magenest List Movie"	/>
                        <resource	id="Magenest_Movie::index" title="Movie index"	/>
                </resource>
                <resource id="Magento_Backend::content">
                    <resource	id="Magenest_Movie::mymenu" title="Technological Option" sortOrder="9999">
                        <resource	id="Magenest_Movie::mypage"	title="Technological Appliances" sortOrder="100">
                            <resource	id="Magenest_Movie::laptoppage"	title="Laptop" sortOrder="100"	/>
                            <resource	id="Magenest_Movie::phonepage"	title="Smart Phone" sortOrder="200"	/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>